<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	
	<context:annotation-config />
	
<!-- 	<bean  -->
<!-- class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/> -->
	
	<bean id="ContactBean" class="com.test.springcore.di.annotatioins.Contact">
		<property name="action" value="buy" />
		<property name="type" value="1" />
	</bean>

	<bean id="PersonBean" class="com.test.springcore.di.annotatioins.Person">
		<property name="name" value="test" />
		<property name="address" value="address 123" />
		<property name="age" value="28" />
	</bean>
	
	<bean id="PersonBean2" class="com.test.springcore.di.annotatioins.Person">
		<property name="name" value="test2" />
		<property name="address" value="address 2" />
		<property name="age" value="28" />
	</bean>
	
	
	</beans>